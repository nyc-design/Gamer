# Local Development Environment Variables
# Copy this to .env and fill in your values

# MongoDB
MONGODB_ATLAS_URI=mongodb://localhost:27017/gamer

# TensorDock API (get both from https://marketplace.tensordock.com/api)
TENSORDOCK_API_KEY=your_tensordock_api_key_here
TENSORDOCK_API_TOKEN=your_tensordock_api_token_here

# Google Cloud
GCP_PROJECT_ID=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# Optional: CloudyPad configuration
CLOUDYPAD_CONFIG_PATH=/path/to/cloudypad/config